<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Channel</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>
  <tbody>
     {{#each videos}}
      <tr>
         <th scope="row">{{sum @index 1}}</th>
         <td>{{this.title}}</td>
         <td>{{this.channel}}</td>
         <td>
            <a href="/admin/{{this._id}}/edit" class="btn btn-dark">
               Edit
            </a>
         </td>
         <td>
            <a href="/admin/{{this._id}}/delete" class="btn btn-danger">
               Delete
            </a>
         </td>
      </tr>
     {{/each}}
  </tbody>
</table>
<a class="btn btn-primary float-start" href="/admin/new">Add</a>